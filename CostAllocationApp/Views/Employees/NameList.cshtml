
@{
    ViewBag.Title = "Name List";
}
<div class="content container-fluid">
    <!--search-->
    <div class="row">
        <div class="col-md-12">
            <table id="name_list_search">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Section</th>
                        <th>Department</th>
                        <th>In-Charge</th>
                        <th>Role</th>
                        <th>Company</th>
                        <th>Status</th>
                    </tr>
                </thead>
